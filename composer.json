{
  "name": "remp/laravel-helpers",
  "description": "Laravel Helpers",
  "keywords": ["laravel", "remp", "helpers"],
  "license": "MIT",
  "authors": [
    {
      "name": "rootpd",
      "email": "dulacka@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/http": "^v8.76.2",
    "illuminate/support": "^v8.76.2",
    "illuminate/routing": "^v8.76.2"
  },
  "require-dev": {
  },
  "autoload": {
    "files": [
      "src/laravel_helpers.php"
    ],
    "psr-4": {
      "Remp\\LaravelHelpers\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Remp\\LaravelHelpers\\Providers\\HelperServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
