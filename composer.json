  "require": {
    "php": "~7.1",
    "illuminate/http": "^5.5",
    "illuminate/support": "^5.5",
    "illuminate/routing": "^5.5"
  },
  "require-dev": {
  },
  "autoload": {
    "files": [
      "src/laravel_helpers.php"
